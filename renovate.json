{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "regexManagers": [
    {
      "fileMatch": [
        "^action\\.yml$"
      ],
      "matchStrings": [
        "default:\\s*['\"](?<currentValue>.*?)['\"]"
      ],
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "dirathea/sstart",
      "extractVersionTemplate": "^v?(?<version>.*)$",
      "versioningTemplate": "semver"
    }
  ]
}
